import java.util.*;
   
   public class Main{
   
   public static void main(String[] args) {
     // write your code here  
      Scanner sc = new Scanner(System.in);
      int n = sc.nextInt();
      int rev = 0;
      while(n!=0)
      {
        int rem = n%10;
        System.out.println(rem);
        
        n/=10;
      }
      
    }
   }
